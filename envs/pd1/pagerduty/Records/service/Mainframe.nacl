pagerduty.service Mainframe {
  name = "Mainframe"
  description = "Managed by Terraform"
  status = "active"
  teams = [
    {
      id = pagerduty.team.instance.IT_Mainframe_Team@s
      type = "team_reference"
    },
  ]
  alert_creation = "create_alerts_and_incidents"
  escalation_policy = {
    id = pagerduty.escalationPolicy.instance.IT_Mainframe_Policy@s
    type = "escalation_policy_reference"
  }
  incident_urgency_rule = {
    type = "constant"
    urgency = "high"
  }
  acknowledgement_timeout = 600
  auto_resolve_timeout = 14400
  integrations = [
    {
      id = "PT14R07"
      type = "generic_email_inbound_integration_reference"
    },
  ]
  type = "service"
  serviceOrchestration = {
    sets = [
      {
        id = "start"
      },
    ]
    type = "service"
  }
}
