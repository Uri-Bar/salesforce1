<style>
#dsDialog {	
	height: 200px; 
	width: 620px; 
	background-image: url('<%= backgroundImage %>');  
	background-repeat: repeat-x; 
	margin: 0; 
	padding: 0;
	border: 0px;
	font-family: Helvetica;
}

#dsHeader {
	height: 46px;
	font-size: 14px;
	color: #20557A;
	font-weight: normal;
}

#dsHeader_right {
	height: 47px;
	float: right;
}

#dsHeader_left {
	height: 47px;
	line-height: 47px;
}

#ds_SelectUserAcct_DialogBox_close {
	width:34px;
	height:46px
}

#dsDialog_main {
	text-align: center;
	height: 173px;
}

.ds_button {
	background-image: url('<%= buttonImage %>');
	cursor:pointer;
	border: none;
	width: 103px;
	height: 36px;
	color: #636363;
	font-size: 12px;
	font-family: Helvetica;
	font-weight: bold;
}
</style>


<div id="dsDialog" >
	<div id="dsHeader">
		<div id="dsHeader_right">
			<input id="ds_SelectUserAcct_DialogBox_close" type="image" src="<%= closeImage %>" alt="close" />
		</div>
		<div id="dsHeader_left">
			<span>&nbsp;&nbsp;</span>
		</div>
	</div>
	<div id="dsDialog_main">
		<br><br>
		<%= headerLabelText %><%= dsUserAcctDropDownHTML %>
		<br><br>
		<button id="ds_select_user_ds_account" class="ds_button" type="button" ><%= dsSelectText %></button>
	</div>
</div>
