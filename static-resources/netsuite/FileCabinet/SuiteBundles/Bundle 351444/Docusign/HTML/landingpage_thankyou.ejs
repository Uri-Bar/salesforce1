<style>
  body {
    color: #333333;
    font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
    font-size: 12px;
    font: normal;
    overflow: scroll;
  }

  .DefaultButton {
    /*width: 280px;*/
    background-color: #357eeb;
    font-family: "Maven Pro Bold", Arial, sans-serif;
    font-size: 14px;
    font-weight: bold;
    color: #ffffff;
    border-radius: 2px;
    border: 1px;
    border-color: #357eeb;
    text-transform: uppercase;
    /*height: 30px;*/
    margin: 20px 0px 20px 0px; /* top right bottom left */
    padding: 9px 18px 9px 18px;
    cursor: pointer;
  }

  .DefaultButton:hover {
    background-color: #2864c6;
    text-decoration: none;
  }
</style>

<div id="dsDialog">
  <table
    id="SignSendDetails"
    style="
      padding: 0px;
      margin: 20px 0px 20px 40px;
      width: 1000px;
      table-layout: fixed;
    "
  >
    <tr>
      <td
        colspan="2"
        style="
          font-size: 22px;
          font-family: Arial;
          font-weight: bolder;
          color: #333333;
        "
      >
        <h3><%= sentText %></h3>
      </td>
    </tr>
    <tr>
      <table
        id="SuccessMessageDetails"
        style="
          padding: 0px;
          margin: 20px 0px 40px 40px;
          width: 600px;
          table-layout: fixed;
        "
      >
        <tr>
          <td
            rowspan="2"
            width="200"
            style="
              font-size: 14px;
              font-family: Arial, Helvetica, sans-serif;
              font-weight: normal;
              color: #333333;
            "
          >
            <img
              src="<%= sentImage %>"
              style="height: 160px; width: 160px; vertical-align: top"
            />
          </td>
          <td align="left" width="500" style="vertical-align: middle">
            <h4><%= successfulSendMessage %></h4>
          </td>
        </tr>
        <tr>
          <td colspan="3" style="margin-bottom: 40px">
            <input
              id="ContinueToSharePointButton"
              type="submit"
              value="Continue to NetSuite"
              class="DefaultButton"
              onclick="window.close()"
            />
          </td>
        </tr>
      </table>
    </tr>
  </table>

  <div
    style="background-color: #e6e6e6; padding-top: 1px; margin-top: 120px"
  ></div>
</div>
